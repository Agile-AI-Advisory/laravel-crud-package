{
    "name": "purrdigital/laravel-crud-package",
    "description": "DO NOT DELETE. CRUD REPOSITORY PATTERN PACKAGE",
    "keywords": [
        "PurrDigital",
        "laravel",
        "laravel-crud-package"
    ],
    "homepage": "https://github.com/PurrDigital/laravel-crud-package",
    "license": "MIT",
    "authors": [
        {
            "name": "James",
            "email": "james.hayes@purrgroup.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "PurrDigital\\LaravelCrudPackage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PurrDigital\\LaravelCrudPackage\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PurrDigital\\LaravelCrudPackage\\LaravelCrudPackageServiceProvider"
            ],
            "aliases": {
                "LaravelCrudPackage": "PurrDigital\\LaravelCrudPackage\\Facades\\LaravelCrudPackage"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
